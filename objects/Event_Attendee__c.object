    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>Attendee__c</fullName>
        <description>Attendee of the Event</description>
        <externalId>false</externalId>
        <inlineHelpText>Select appropriate Event attendee</inlineHelpText>
        <label>Attendee</label>
        <referenceTo>Attendee__c</referenceTo>
        <relationshipLabel>Event Attendees</relationshipLabel>
        <relationshipName>Event_Attendees</relationshipName>
        <relationshipOrder>1</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Event_ProFit__c</fullName>
        <description>Defines the event</description>
        <externalId>false</externalId>
        <inlineHelpText>Select appropriate Event</inlineHelpText>
        <label>Event(ProFit)</label>
        <referenceTo>Event_Profit__c</referenceTo>
        <relationshipLabel>Event Attendees</relationshipLabel>
        <relationshipName>Event_Attendees</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <label>Event Attendee</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <displayFormat>EA-{0000000}</displayFormat>
        <label>Event Attendee Name</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Event Attendees</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <startsWith>Vowel</startsWith>
    <validationRules>
        <fullName>CheckSeatsValidationRule</fullName>
        <active>true</active>
        <description>Seats must have extra</description>
        <errorConditionFormula>Event_ProFit__r.Remaining_Seats__c  = 0</errorConditionFormula>
        <errorDisplayField>Event_ProFit__c</errorDisplayField>
        <errorMessage>Check seats availability</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EventAttendee1DayValidationRule</fullName>
        <active>true</active>
        <description>Attendee can only be associated with the Event whose End Date is in future</description>
        <errorConditionFormula>DATETIMEVALUE( Event_ProFit__r.End_Date_Time__c )&lt;=DATETIMEVALUE(TODAY())</errorConditionFormula>
        <errorDisplayField>Event_ProFit__c</errorDisplayField>
        <errorMessage>End of the event must be in future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EventMustLive</fullName>
        <active>true</active>
        <description>Event MUST be LIVE</description>
        <errorConditionFormula>Event_ProFit__r.Live__c  = FALSE</errorConditionFormula>
        <errorDisplayField>Event_ProFit__c</errorDisplayField>
        <errorMessage>Event MUST be LIVE</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
