    <validationRules>
        <fullName>EventTypeEqualsVirtualRule</fullName>
        <active>true</active>
        <description>If Virtual is Selected as Value for Event Type field then PREVENT user to Select Location on Event Record</description>
        <errorConditionFormula>AND( ISPICKVAL(   Event__c   , &apos;Virtual&apos;) ,   NOT(ISBLANK(  Location__c   ) ) )</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>If the event is Virtual, Location is not available</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EventTypeInPersonRule</fullName>
        <active>true</active>
        <description>If Event Type is In-Person then user is required to select Location on Event Record.</description>
        <errorConditionFormula>AND(ISPICKVAL( Event__c ,&apos;In-person&apos;),ISBLANK( Location__c ))</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>Location is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurringRule</fullName>
        <active>true</active>
        <description>If Recurring is checked, the user must need to fill Frequency field. and If Recurring is not checked, Frequency must be &quot;None&quot;</description>
        <errorConditionFormula>OR(AND( Recurring__c  = TRUE, ISBLANK( TEXT(  Frequency__c  ))),AND(Recurring__c  = FALSE , NOT(ISBLANK( TEXT( Frequency__c )))))</errorConditionFormula>
        <errorMessage>If Event is Recurring, pick Frequency field and If Event is not Recurring, Frequency field must be &quot;None&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDateTimeOneDayRule</fullName>
        <active>true</active>
        <description>End Date/Time must be at least 1 day ahead of Start Date/Time</description>
        <errorConditionFormula>AND(DATEVALUE(   End_Date_Time__c   ) &lt; (DATEVALUE(   Start_Date_Time__c   ) + 1), NOT( ISNULL(  End_Date_Time__c  ) ) )</errorConditionFormula>
        <errorDisplayField>End_Date_Time__c</errorDisplayField>
        <errorMessage>End Date must be at least 1 day ahead from Start Date</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
